{
  "name": "bookworm-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "smui-css": "find node_modules/@smui -name 'bare.css' | xargs cat >  src/lib/css/smui.css",
    "preview": "svelte-kit preview",
    "prepare": "npm run vite-workaround && npm run smui-css",
    "vite-workaround": "find node_modules/@material/ -name \"*.js.map\" -type f -delete",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "eslint": "^7.22.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "fast-deep-equal": "^3.1.3",
    "material-icons": "^0.7.6",
    "smui-theme": "^4.2.0",
    "svelte": "^3.34.0",
    "svelte-vega": "^1.1.0",
    "vega": "^5.20.2",
    "vega-lite": "^5.1.0"
  },
  "type": "module",
  "dependencies": {
    "@smui/button": "^4.2.0",
    "@smui/chips": "^4.2.0",
    "@smui/dialog": "^4.2.0",
    "@smui/form-field": "^4.2.0",
    "@smui/icon-button": "^4.2.0",
    "@smui/list": "^4.2.0",
    "@smui/menu": "^4.2.0",
    "@smui/menu-surface": "^4.2.0",
    "@smui/paper": "^4.2.0",
    "@smui/radio": "^4.2.0",
    "@smui/select": "^4.2.0",
    "@smui/snackbar": "^4.2.0",
    "@smui/tab": "^4.2.0",
    "@smui/tab-bar": "^4.2.0",
    "@smui/textfield": "^4.2.0",
    "@smui/top-app-bar": "^4.2.0",
    "bookworm-vega": "file:../bookworm-vega",
    "npm": "^7.20.0",
    "svelte-loading-spinners": "^0.1.6"
  }
}
